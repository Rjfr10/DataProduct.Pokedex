{
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "SwaggerUI": {
    "Name": "Pokemon Api",
    "Endpoint": "/swagger/v1/swagger.json"
  },
  "ExternalSource": {
    "FunTranslations": "https://api.funtranslations.com/translate"
  },
  "Variables": {
    "TranslateEnabled": true,
    "HabitatsToTranslateToYoda": [ "cave" ],
    "LegendaryTranslateToYoda": true,
    "FlavorTextLanguage": "en"
  }
}